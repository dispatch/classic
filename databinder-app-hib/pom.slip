"""<?xml version="1.0" encoding="UTF-8" ?>"""
project(xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance",xmlns="http://maven.apache.org/POM/4.0.0",xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd"):

	modelVersion(): "4.0.0"
	artifactId: "databinder-app-hib"
	name: "Databinder appkit / Hibernate"
	parent:
		groupId: "net.databinder"
		artifactId: "databinder-parent"
		version: "1.2.1"

	dependencies:
		dependency:
			groupId: "${pom.groupId}"
			version: "${pom.version}"
			artifactId: "databinder-auth-components-hib"

		dependency:
			groupId: "${pom.groupId}"
			version: "${pom.version}"
			artifactId: "databinder-app"

		dependency:
			groupId: "javax.servlet"
			artifactId: "servlet-api"
			version: "2.3"
			scope: "provided"
		
		dependency:
			groupId: "org.hibernate"
			artifactId: "hibernate-annotations"
			version: "3.4.0.GA"

		dependency:
			groupId: "org.hibernate"
			artifactId: "hibernate-core"
			version: "3.3.1.GA"

		dependency:
			groupId: "javassist"
			artifactId: "javassist"
			version: "3.4.GA"

		dependency:
			groupId: "org.hibernate"
			artifactId: "hibernate-c3p0"
			version: "3.3.1.GA"

	repositories:
		repository:
			id: "com.jboss"
			url: "http://repository.jboss.com/maven2/"
